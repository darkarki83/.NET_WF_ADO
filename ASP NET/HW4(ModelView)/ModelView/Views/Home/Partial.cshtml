@model List<ModelView.Models.Product>

@{ foreach (var item in Model)
    {
        <dl class="row" style="width:30%">
            <dd class="col-sm-10">
                @Html.DisplayFor(Model => item.Id) .
                @Html.DisplayFor(Model => item.Name)  -
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayFor(Model => item.Price)
            </dt>
        </dl>
    }
}




