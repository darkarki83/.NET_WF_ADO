<Window x:Class="ExSmile2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ExSmile2"
        mc:Ignorable="d"
        Title="MainWindow" Height="250" Width="400" FontSize="20">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Button Grid.Row="0" Name="one" Content="windows 1" Background="{DynamicResource {x:Static SystemColors.WindowTextBrushKey}}" Click="windows_1_Click"></Button>
        <Button Grid.Row="1" Content="windows 2" Click="windows_2_Click"></Button>
        <Button Grid.Row="2" Content="find" Click="find_Click"></Button>
    </Grid>
</Window>
